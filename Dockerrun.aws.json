{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "command": ["sh", "-c", "npm install && npm run prod:start"],
      "essential": true,
      "image": "node:14-buster",
      "memory": 200,
      "mountPoints": [
        {
          "containerPath": "/caturday-be",
          "sourceVolume": "_"
        }
      ],
      "name": "caturday-be",
      "portMappings": [
        {
          "containerPort": 4000,
          "hostPort": 4000
        }
      ],
      "workingDirectory": "/caturday-be"
    }
  ],
  "family": "",
  "volumes": [
    {
      "host": {
        "sourcePath": "./"
      },
      "name": "_"
    }
  ]
}
